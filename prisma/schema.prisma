generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL") 
  directUrl = env("DIRECT_URL") 
}

model Guestbook {
  id        String   @id @default(cuid())
  name      String
  message   String
  ipAddress String?
  createdAt DateTime @default(now())
}